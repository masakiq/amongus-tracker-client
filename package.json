  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/masakiq/amongus-tracker-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/masakiq/amongus-tracker-client/issues"
  },
  "homepage": "https://github.com/masakiq/amongus-tracker-client#readme",
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "discord-rpc": "github:masakiq/RPC",
    "ws": "^7.5.3"
  },
  "build": {
    "appId": "com.amongus-tracker",
    "files": [
      "package.json",
      "package-lock.json",
      "src/main.js"
    ],
    "mac": {
      "target": "dmg",
